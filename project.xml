<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="Stgldemo" package="com.abagames.stgldemo" version="1.0.0" company="Company Name" />
	
	<app main="com.abagames.stgldemo.Main" path="Export" file="Stgldemo" />

	<window fps="60" background="0x333333" />
	<window width="640" height="480" resizable="true" unless="mobile" />
	<window fullscreen="true" if="mobile" />
	<window orientation="landscape" vsync="true" antialiasing="0" if="cpp" />
	
	<source path="Source" />
	
	<haxelib name="openfl" />
	
	<assets path="Assets" rename="assets" exclude="openfl.svg" />
	<icon path="Assets/openfl.svg" />
	
</project>